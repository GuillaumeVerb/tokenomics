{
    "venv": "venv",
    "venvPath": ".",
    "pythonPath": "./venv/bin/python",
    "typeCheckingMode": "basic",
    "useLibraryCodeForTypes": true,
    "reportMissingImports": "warning",
    "extraPaths": [".", "backend"],
    "pythonVersion": "3.9",
    "analysis": {
        "autoImportCompletions": true,
        "useLibraryCodeForTypes": true,
        "diagnosticMode": "workspace",
        "autoSearchPaths": true
    }
} 